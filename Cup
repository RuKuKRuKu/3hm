package com.company;

public class Cup extends Dish
{
    protected int volume;

    Cup(int v,int p)
    
    {
        volume=v;
        price=p;
    }
    
    @Override
    
    void setProperty(int v)
    
    {
        this.volume = v;
    }
    
    @Override
    
    int getProperty()
    
    {
        return volume;
    }

    @Override
    
    public String toString()
    
    {
        return "volume = " + volume +
                ", price = " + price;
    }
}
